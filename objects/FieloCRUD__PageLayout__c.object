    <webLinks>
        <fullName>FieloCRUD__DeepClone</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Deep Clone</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/30.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/30.0/apex.js&apos;)}

var output = sforce.apex.execute(&quot;FieloCRUD.PageLayoutCloneButton&quot;, &quot;doClone&quot;, {pageLayoutId:&quot;{!FieloCRUD__PageLayout__c.Id}&quot;});

if(String(output).indexOf(&apos;ID&apos;) == 0){
   window.location = &quot;/&quot; + String(output).slice(3);
}else{
   alert( output );
}</url>
    </webLinks>
