    <validationRules>
        <fullName>FieloCRUD__PageLayout_ProfileOnlyForMember</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	RecordType.DeveloperName = &apos;Profile&apos;,
	FieloCRUD__ObjectSettings__r.FieloCRUD__ObjectAPIName__c &lt;&gt; &apos;FieloEE__Member__c&apos;
)</errorConditionFormula>
        <errorMessage>Profile record type is only available for object settings related to &quot;FieloEE__Member__c&quot; object.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCRUD__PageLayout_Type</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(FieloCRUD__ValueToDefinePageLayout__c))
&amp;&amp;
ISBLANK(FieloCRUD__ObjectSettings__r.FieloCRUD__FieldToDefinePageLayout__c)</errorConditionFormula>
        <errorDisplayField>FieloCRUD__ValueToDefinePageLayout__c</errorDisplayField>
        <errorMessage>Please complete the field &quot;Field To Define Page Layout&quot; in the related &quot;Object Settings&quot; record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCRUD__PageLayout_TypeIsMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	FieloCRUD__Default__c = FALSE,
	NOT(ISBLANK(FieloCRUD__ObjectSettings__r.FieloCRUD__FieldToDefinePageLayout__c )),
	ISBLANK(FieloCRUD__ValueToDefinePageLayout__c),
	RecordType.DeveloperName = &apos;Standard&apos;
)</errorConditionFormula>
        <errorDisplayField>FieloCRUD__ValueToDefinePageLayout__c</errorDisplayField>
        <errorMessage>You must include type if the object setting has a field that defines layout.</errorMessage>
    </validationRules>
