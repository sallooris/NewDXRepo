    <validationRules>
        <fullName>FieloCH__Edit_Reward</fullName>
        <active>true</active>
        <description>If the Challenge is active then the reward cannot be modified</description>
        <errorConditionFormula>FieloCH__Challenge__r.FieloCH__IsActive__c</errorConditionFormula>
        <errorMessage>Cannot modified the Reward if the Challenge is Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Reward_Program_Validation</fullName>
        <active>true</active>
        <description>Checks that the reward belongs to the same program as the challenge.</description>
        <errorConditionFormula>AND(
    NOT( ISBLANK(FieloCH__Reward__c) ) ,
    FieloCH__Reward__r.FieloEE__Program__c !=  FieloCH__Challenge__r.FieloCH__Program__c)</errorConditionFormula>
        <errorMessage>The Reward does not belong to the same Program as the Challenge.</errorMessage>
    </validationRules>
