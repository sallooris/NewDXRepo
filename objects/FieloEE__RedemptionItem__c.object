{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var prefix = &apos;{!$Setup.FieloEE__PublicSettings__c.FieloEE__PackagePrefix__c}&apos;.replace(&apos;__&apos;, &apos;.&apos;);

