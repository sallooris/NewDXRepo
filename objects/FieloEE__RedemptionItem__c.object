    <webLinks>
        <fullName>FieloEE__DeliverItems</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Deliver Items</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var itemsSelected = {!GETRECORDIDS( $ObjectType.FieloEE__RedemptionItem__c ) };

if(itemsSelected.length == 0){
    alert(&apos;{!$Label.fieloee__itemsrequired}&apos;);
}else{

var prefix = &apos;{!$Setup.FieloEE__PublicSettings__c.FieloEE__PackagePrefix__c}&apos;.replace(&apos;__&apos;, &apos;.&apos;);

var status = sforce.apex.execute( prefix+&apos;RedemptionUtil&apos;, &apos;deliverItems&apos;, {id: &apos;{!FieloEE__Redemption__c.Id}&apos;, itemsIds: itemsSelected });

alert(status);

location.reload();
}</url>
    </webLinks>
