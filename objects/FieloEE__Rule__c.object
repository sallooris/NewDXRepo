    <validationRules>
        <fullName>FieloEE__Rule_Name_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Name )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name must be completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloEE__Validate_points_value</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISPICKVAL(FieloEE__ConvertionTypeTransaction__c,&apos;Fix Value&apos;), ISPICKVAL(FieloEE__ConvertionTypeTransaction__c,&apos;Factor&apos;), ISPICKVAL(FieloEE__ConvertionTypeTransaction__c,&apos;Multiplier&apos;) ), OR(FieloEE__ConvertionValue__c == 0, ISNULL(FieloEE__ConvertionValue__c), ISBLANK(FieloEE__ConvertionValue__c)) )</errorConditionFormula>
        <errorMessage>The quantity of points cannot be empty or be 0</errorMessage>
    </validationRules>
